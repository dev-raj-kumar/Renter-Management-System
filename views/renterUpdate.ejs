<%- include('header') -%>
 <form class="" action="updateRenter" method="post">
   <input type="hidden" name="id" value="<%= id %>">
    Email : <input type="text" name="email" value="<%= email %>" > <br>
    Address : <input type="text" name="address" value="<%= address %>" > <br>
    Monthly Rent : <input type="number" name="monthlyrent" value="<%= monthlyrent %>" step="0.01" min="0"> <br>
    Electricity Bill Rate(per unit) : <input type="number" name="rateelec" value="<%= rateelec %>" step="0.01" min="0"> <br>
    Water Bill Rate(per unit) : <input type="number" name="ratewater" value="<%= ratewater %>" step="0.01" min="0"> <br>
    <button type="submit" name="button">Update</button>
 </form>
<%- include('footer') -%>
